        <WaveformRange>
            <Range name="pspmt:dynode">
                <Low value="5"/>
                <High value="12"/>
            </Range>
        </WaveformRange>
    </Trace>

    <Fitting>
        <SigmaBaselineThresh value="3.0"/>
        <Parameters>
            <Pars name="vandle:medium">
